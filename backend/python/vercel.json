{
  "version": 2,
  "builds": [
    {
      "src": "api/problems.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/problems/(.*)",
      "dest": "/api/problems.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/problems.py"
    }
  ]
} 